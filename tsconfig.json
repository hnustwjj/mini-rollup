  "compilerOptions": {
    "target": "es2016",
    "module": "CommonJS",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "moduleResolution": "NodeNext",
    "skipLibCheck": true,
    "noImplicitAny": false,
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
  },
}
